<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="item"
            type="com.pax.mvvmsample.ui.wanandroid.navigation.NaviItemViewModel"/>

    </data>
    <TextView
        android:id="@+id/tvChapterName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/select_navi_bg"
        android:textColor="@color/select_navi_text"
        android:padding="10dp"
        android:textSize="16dp"
        android:text="@{item.chapterName}"/>

</layout>
